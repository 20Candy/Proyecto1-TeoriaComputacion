// Minimizacion
digraph {
	rankdir=LR size=15
	label="\AFD: Minimizacion [(a|b)*abb]"
	fontsize=20
	node [shape=circle]
	"" [height=0 shape=none width=0]
	0 [label=0]
	"" -> 0
	1 [label=1]
	2 [label=2]
	3 [label=3 shape=doublecircle]
	0 -> 1 [label=a]
	0 -> 0 [label=b]
	1 -> 1 [label=a]
	1 -> 2 [label=b]
	2 -> 1 [label=a]
	2 -> 3 [label=b]
	3 -> 1 [label=a]
	3 -> 0 [label=b]
}
